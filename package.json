{
  "name": "sulcalc",
  "version": "0.4.0",
  "author": "sulcata",
  "private": true,
  "description": "A Pokemon damage calculator.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sulcata/sulcalc"
  },
  "bugs": {
    "url": "https://github.com/sulcata/sulcalc/issues"
  },
  "scripts": {
    "serve":
      "webpack-dev-server --config build/webpack.app.config.js --mode development",
    "build": "webpack --config build/webpack.app.config.js --mode production",
    "build:lib":
      "webpack --config build/webpack.lib.config.js --mode production",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest --coverage",
    "lint": "eslint '**/*.{js,vue}' && npm run style -- --list-different",
    "style:fix": "npm run style -- --write",
    "style": "prettier '**/*.{js,json,md,vue}' --ignore-path .eslintignore",
    "setup": "npm run db && npm run setdex",
    "db": "babel-node scripts/db",
    "setdex": "babel-node scripts/setdex"
  },
  "dependencies": {
    "big-integer": "^1.6.27",
    "bootstrap": "^4.0.0",
    "lodash": "^4.17.5",
    "vue": "^2.5.16",
    "vue-multiselect": "^2.0.8",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-promise": "^3.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fs-extra": "^5.0.0",
    "handlebars": "^4.0.10",
    "handlebars-loader": "^1.7.0",
    "html-webpack-plugin": "^3.1.0",
    "jest": "^22.4.3",
    "prettier": "^1.11.1",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "style-loader": "^0.20.3",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  }
}
