{
  "name": "sulcalc",
  "version": "0.5.0",
  "author": "sulcata",
  "private": true,
  "description": "A Pokemon damage calculator.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sulcata/sulcalc"
  },
  "bugs": {
    "url": "https://github.com/sulcata/sulcalc/issues"
  },
  "scripts": {
    "serve": "webpack-serve --config build/webpack.app.config.js --no-clipboard",
    "build": "webpack --config build/webpack.app.config.js",
    "build:lib": "webpack --config build/webpack.lib.config.js",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest --coverage",
    "lint": "eslint '**/*.{js,vue}' && npm run style -- --list-different",
    "style:fix": "npm run style -- --write",
    "style": "prettier '**/*.{js,json,md,vue,yml,yaml}' --ignore-path .eslintignore",
    "setup": "npm run db && npm run stats && npm run setdex",
    "db": "babel-node scripts/db",
    "stats": "babel-node scripts/stats",
    "setdex": "babel-node scripts/setdex"
  },
  "dependencies": {
    "big-integer": "^1.6.36",
    "bootstrap": "^4.1.3",
    "lodash": "^4.17.10",
    "vue": "^2.5.17",
    "vue-multiselect": "2.0.8",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.2",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-promise": "^4.0.1",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "mini-css-extract-plugin": "^0.4.2",
    "prettier": "^1.14.2",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "terser-webpack-plugin": "^1.0.2",
    "vue-loader": "^15.4.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-serve": "^2.0.2"
  }
}
